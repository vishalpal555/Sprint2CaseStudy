#%RAML 1.0 DataType
type: array
items:
  type: object
  properties:
    departureDateTime:
      type: string
      example: "2019-04-30T16:41:41.090Z"
    fromLocation:
      type: string
      example: "BEN-SG"
    toLocation: 
      type: string
      example: "KLG-MY"
    availableSeats:
      type: number
      example: 50